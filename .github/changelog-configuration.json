{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature"]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": ["fix"]
    },
    {
      "title": "## 🧪 Tests",
      "labels": ["test"]
    },
    {
      "title": "## 💬 Other / Chores",
      "labels": ["other", "chore"]
    }
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "no changes",
  "max_pull_requests": 1000,
  "max_back_track_time_days": 1000
}
